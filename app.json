{
  "expo": {
    "name": "Mazinda",
    "slug": "HelloWorld",
    "version": "1.4.2",
    "scheme": "mazinda",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.Mazinda.MazindaApp",
      "infoPlist": {
        "UIBackgroundModes": ["location", "fetch", "remote-notification"]
      },
      "buildNumber": "4"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.abhey_gupta.MazindaApp",
      "versionCode": 8
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ab9c3df0-4476-4644-9699-4b8133795b5f"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-updates",
        {
          "username": "abhey_gupta"
        }
      ]
    ],
    "runtimeVersion": "1.4.2",
    "updates": {
      "url": "https://u.expo.dev/ab9c3df0-4476-4644-9699-4b8133795b5f"
    }
  }
}
